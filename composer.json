{
    "name": "yurii/sage",
    "description": "CLI helper for local development",
    "keywords": ["brew", "console", "cli", "mamp", "sage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yurii Torbyk",
            "email": "mail@yurii.me"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "5.7.*",
        "symfony/yaml": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["sage"]
}
