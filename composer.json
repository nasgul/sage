{
    "name": "yurii/swarm",
    "description": "CLI helper for local development",
    "keywords": ["brew", "console", "cli", "mamp", "swarm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yurii Torbyk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["swarm"]
}
